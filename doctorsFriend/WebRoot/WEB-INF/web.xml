<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>doctorsFriend</display-name>
  <context-param>
    <param-name>datasource</param-name>
    <param-value>ds-datasource</param-value>
  </context-param>
  <context-param>
    <param-name>DBType</param-name>
    <param-value>db_pgsql</param-value>
  </context-param>
  <context-param>
    <param-name>configFile</param-name>
    <param-value>\webapps\container\config\startupConfig.xml</param-value>
  </context-param>
  <context-param>
    <param-name>operatingSystem</param-name>
    <param-value>windows</param-value>
  </context-param>
  <context-param>
    <param-name>License</param-name>
    <param-value>false</param-value>
  </context-param>
  <welcome-file-list>
    <welcome-file>form/common/login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ConfigServlet</servlet-name>
    <servlet-class>servlets.ConfigServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConfigServlet</servlet-name>
    <url-pattern>/servlet/ConfigServlet</url-pattern>
  </servlet-mapping>
</web-app>